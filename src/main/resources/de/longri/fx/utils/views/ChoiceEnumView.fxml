<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="fxRoot" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox spacing="10.0">
         <children>
            <HBox alignment="CENTER" VBox.vgrow="NEVER">
               <children>
                  <Label fx:id="fxLblChoiceText" alignment="CENTER" contentDisplay="CENTER" text="Choice the export type" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <ChoiceBox fx:id="fxChoice" prefWidth="150.0" />
            <HBox spacing="10.0">
               <children>
                  <Pane HBox.hgrow="ALWAYS" />
                  <Button fx:id="fxBtnCancel" mnemonicParsing="false" onAction="#OnCancelClicked" text="Cancel" />
                  <Button fx:id="fxBtnOK" mnemonicParsing="false" onAction="#onOkClicked" text="OK" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
